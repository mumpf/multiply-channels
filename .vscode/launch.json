    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Sensormodule",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/MultiplyChannels.dll",
            "args": [
                "create",
                "--Debug",
                "../Projects/knx-sensor/src/Sensormodul"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Debug WireGateway",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/MultiplyChannels.dll",
            "args": [
                "create",
                "--Debug",
                "../Projects/knx-wire/src/WireGateway"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Debug Logicmodule",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/MultiplyChannels.dll",
            "args": [
                "create",
                "--Debug",
                "../OpenKNX/OAM-LogicModule/src/Logikmodul"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Debug Matrix",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/MultiplyChannels.dll",
            "args": [
                "create",
                "--Debug",
                "../Projects/knx-matrix/src/Matrix"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Debug Check",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/MultiplyChannels.dll",
            "args": [
                "new",
                "--AppName=AppName",
                "--AppNumber=543",
                "--SerialNumber=SERIAL",
                "--OrderNumber=ORDER",
                "--ProductName=ProductName",
                "--HardwareName=HardwareName",
                "--HardwareVersion=12",
                "--Debug",
                "Check.xml"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Debug Temp File",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/MultiplyChannels.dll",
            "args": [
                "create",
                "--Debug",
                "bin/Debug/netcoreapp3.1/temp.xml"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}